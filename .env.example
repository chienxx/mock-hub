# 数据库配置
DATABASE_URL="mysql://root:password@localhost:3306/mockdb"

# Redis配置
REDIS_URL="redis://localhost:6379/0"
REDIS_PASSWORD=""  # 可选：单独配置密码

# NextAuth配置
NEXTAUTH_URL="http://localhost:3000"
NEXTAUTH_SECRET="your-secret-key-at-least-32-chars-use-openssl-rand-base64-32"

# 系统配置
NODE_ENV="development"
PORT=3000

# JWT配置
JWT_SECRET="your-jwt-secret-key"
JWT_EXPIRES_IN="7d"

IGNORE_SSL_ERRORS=true

# 日志清理配置
LOG_RETENTION_DAYS=90  # API日志保留天数，默认90天
